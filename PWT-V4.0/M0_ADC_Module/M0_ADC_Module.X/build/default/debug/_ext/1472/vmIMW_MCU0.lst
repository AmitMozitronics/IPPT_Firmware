....................      
....................     // Initialize array member. 
....................     for(ui8LoopCount = 0; ui8LoopCount < SIZE_AVG_WT_ARR; ui8LoopCount++) 
....................     { 
....................         g_stAvgWtQueue.m_i32Arr[ui8LoopCount] = 0; 
